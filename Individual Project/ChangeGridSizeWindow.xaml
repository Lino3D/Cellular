<Window x:Class="Individual_Project.ChangeGridSizeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Change Grid" Height="197" Width="259" Closing="Window_Closing">
    <Grid Margin="0,0,2,-3">
        <TextBox  x:Name="NewXBox" HorizontalAlignment="Left" Height="23" Margin="35,33,0,0" TextWrapping="Wrap" Text="20" VerticalAlignment="Top" Width="50" />
        <TextBox  x:Name="NewYBox" HorizontalAlignment="Left" Height="23" Margin="188,33,0,0" TextWrapping="Wrap" Text="20" VerticalAlignment="Top" Width="51" RenderTransformOrigin="0.358,0.217"/>
        <Label Content="X" HorizontalAlignment="Left" Margin="10,33,0,0" VerticalAlignment="Top"/>
        <Label Content="Y" HorizontalAlignment="Left" Margin="156,33,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.237,0"/>
        <Button Content="Accept" HorizontalAlignment="Left" Margin="10,127,0,0" VerticalAlignment="Top" Width="75" Click="Accept_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="164,127,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <TextBox x:Name="Check" HorizontalAlignment="Left" Height="23" Margin="91,81,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Visibility="Hidden" />

    </Grid>
</Window>
